# assigning elements for different lists
fruits = ("mango","orange","berry","cherry")
veggies = ("tomato","potato","onion","beans")
# accesing elements in tuples
myTuple = ("apple","mango","orange")
print(myTuple[2])
# deleting different dictionary elements
myDict = {
    "Kohli": "Captain",
    "Rohit sharma": "Vice captain",
    "Dhoni": "Ice captain"
    }
 # removing value
 del myDict["kohli"]
 del myDict
